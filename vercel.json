{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/feedback.ts": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/feedback/stats.ts": {
      "memory": 128,
      "maxDuration": 5
    }
  }
}
